#include <stdio.h>

int main() {
    int numero, divisor;

    printf("Digite o número: ");
    scanf("%d", &numero);

    printf("Digite o possível divisor: ");
    scanf("%d", &divisor);

    if (divisor == 0) {
        printf("Divisão por zero não é permitida.\n");
    } else if (numero % divisor == 0) {
        printf("%d é múltiplo de %d.\n", numero, divisor);
    } else {
        printf("%d não é múltiplo de %d.\n", numero, divisor);
    }

    return 0;
}
